{
  "name": "@aidenlx/chs-patch",
  "version": "1.5.0",
  "description": "A patch for Obsidian's built-in CodeMirror Editor to support Simplified Chinese word splitting",
  "private": true,
  "scripts": {
    "dev": "cross-env BUILD=dev node esbuild.config.mjs",
    "check": "tsc --noEmit",
    "build": "cross-env BUILD=production node esbuild.config.mjs",
    "prettier": "prettier --write 'src/**/*.+(ts|tsx|json|html|css)'",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "release": "release-it"
  },
  "keywords": [],
  "author": "AidenLx",
  "license": "MIT",
  "devDependencies": {
    "@aidenlx/esbuild-plugin-obsidian": "^0.1.4",
    "@aidenlx/eslint-config": "^0.1.2",
    "@aidenlx/obsidian-plugin-bumper": "^0.1.3",
    "@aidenlx/prettier-config": "^0.1.0",
    "@aidenlx/release-it-config": "^0.1.7",
    "@aidenlx/ts-config": "^0.1.5",
    "@codemirror/state": "^0.19.9",
    "@codemirror/view": "0.19.44",
    "@release-it/bumper": "^3.0.1",
    "@release-it/conventional-changelog": "^4.1.0",
    "@types/codemirror": "^5.60.5",
    "@types/node": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "assert-never": "^1.2.1",
    "cm6-view-src": "github:aidenlx/view",
    "conventional-changelog-angular": "^5.0.13",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.13.15",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.9.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "file-select-dialog": "^1.5.4",
    "jieba-wasm": "^0.0.2",
    "monkey-around": "^2.3.0",
    "obsidian": "github:obsidianmd/obsidian-api",
    "prettier": "^2.5.1",
    "release-it": "^14.12.5",
    "semver": "^7.3.5",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@3.2.0"
}
